{
  "name": "hardal",
  "version": "3.6.9",
  "description": "Connect your first-party data from any source to any destination for websites and mobile apps with server-side.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.js",
      "require": "./dist/react/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "react": ["./dist/react/index.d.ts"]
    }
  },
  "scripts": {
    "build": "tsc && npm run build:browser",
    "build:browser": "webpack --mode production --config webpack.browser.config.js",
    "build:types": "tsc",
    "test": "jest",
    "prepare": "npm run build"
  },
  "keywords": [
    "analytics",
    "tracking",
    "privacy"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.19.76",
    "@types/react": "^18.2.0",
    "react": "^18.2.0",
    "ts-loader": "^9.5.2",
    "typescript": "^4.9.5",
    "webpack": "^5.98.0",
    "webpack-cli": "^5.1.4"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "files": [
    "dist",
    "README.md",
    "REACT.md",
    "NEXTJS_SETUP.md",
    "PERFORMANCE_TIPS.md"
  ]
}